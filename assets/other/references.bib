  pages = {1439--1463},
  publisher = {{Society for Judgment and Decision Making; https://web.archive.org/web/20220121141838/https://ideas.repec.org/a/jdm/journl/v16y2021i6p1439-1463.html}},
  url = {https://ideas.repec.org/a/jdm/journl/v16y2021i6p1439-1463.html},
  urldate = {2022-01-21},
  abstract = {Many popular books and articles that purport to explain how people, companies, or ideas succeed highlight a few successes chosen to fit a particular narrative. We investigate what effect these highly selected â€œsuccess narrativesâ€ have on readers' beliefs and decisions. We conducted a large, randomized, pre-registered experiment, showing participants successful firms with founders that all either dropped out of or graduated college, and asked them to make incentive-compatible bets on a new firm. Despite acknowledging biases in the examples, participants' decisions were very strongly influenced by them. People shown dropout founders were 55 percentage points more likely to bet on a dropout-founded company than people who were shown graduate founders. Most reported medium to high confidence in their bets, and many wrote causal explanations justifying their decision. In light of recent concerns about false information, our findings demonstrate how true but biased information can strongly alter beliefs and decisions.},
  langid = {english},
  keywords = {communication,decision-making,selection bias},
  file = {E\:\\OneDrive\\Documents\\Archive\\zotero\\Lifchits_etal_2021_Success_stories_cause_false_beliefs_about_success.pdf;C\:\\Users\\Je Sian Keith Herman\\Zotero\\storage\\4WAVLPPJ\\v16y2021i6p1439-1463.html}
}

