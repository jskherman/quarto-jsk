  author = {Pilavtepe, Muge and Yesil-Celiktas, Ozlem},
  date = {2013-10-01},
  journaltitle = {The Journal of Supercritical Fluids},
  shortjournal = {The Journal of Supercritical Fluids},
  volume = {82},
  pages = {244--250},
  issn = {0896-8446},
  doi = {10.1016/j.supflu.2013.07.020},
  url = {https://www.sciencedirect.com/science/article/pii/S0896844613002738},
  urldate = {2021-10-08},
  abstract = {Posidonia oceanica residues were extracted with supercritical CO2 in order to isolate phenolic compounds. The process was optimized by developing a mathematical model based on mass transfer mechanism consisting of adsorption of supercritical fluid on the solid particles, desorption of solute and convective transfer of solute phase along the column. Henry relation between solute concentrations on the surface of the solid (Cs) and in the solid (q) was approximated in order to describe the adsorption/desorption equilibrium. The model parameters such as solid-liquid film mass transfer coefficient (kf), molecular diffusivity coefficient (DAB) and axial dispersion (Dax) were estimated using empirical methods. The linear driving force model was applied to improve the yield of total phenolic acid recovery. The optimum parameters were elicited as 25MPa, 323.15K and a co-solvent mass ratio of 20\% yielding 34.97μg per gram of dry feed and the model satisfactorily described the extraction yield which can be used for scale-up purposes.},
  langid = {english},
  keywords = {Environmental pollution,Mass transfer,Mathematical modeling,Phenolic compounds,Supercritical fluid extraction},
  file = {C\:\\Users\\Je Sian Keith Herman\\Zotero\\storage\\BGIA6Z83\\Pilavtepe and Yesil-Celiktas - 2013 - Mathematical modeling and mass transfer considerat.pdf;E\:\\OneDrive\\Documents\\Archive\\zotero\\Pilavtepe-Yesil-Celiktas_2013_Mathematical_modeling_and_mass_transfer_considerations_in_supercritical_fluid.pdf}
}

