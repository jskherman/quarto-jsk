  title = {Rapid Estimation of the Manufacturing Cost of Extracts Obtained by Supercritical Fluid Extraction},
  author = {Rosa, Paulo T. V. and Meireles, M. Angela A.},
  date = {2005-03-01},
  journaltitle = {Journal of Food Engineering},
  shortjournal = {Journal of Food Engineering},
  series = {{{IV Iberoamerican Congress}} of {{Food Engineering}} ({{CIBIA IV}})},
  volume = {67},
