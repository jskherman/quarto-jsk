@article{fernandes2011,
  title = {Modelling and {{Simulation}} of a {{Complete Supercritical Fluid Extraction Plant}} with {{Countercurrent Fractionation Column}}},
  author = {Fernandes, João Beja and Lisboa, Pedro Ferreira and Barbosa Mota, José Paulo and Simões, Pedro Calado},
  date = {2011-08-01},
  journaltitle = {Separation Science and Technology},
  volume = {46},
