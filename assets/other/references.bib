@thesis{albuero2021,
  type = {bathesis},
  title = {Design and {{Fabrication}} of {{Essential Oil Disitiller}} for {{Pili}} ({{Canarium}} Luzonicum) {{Resin}}},
  author = {Albuero, Raymond and Baltazar, Jan Carlo and de Lima, Mary Rose and Fabia, Genesis and Locsin, Ela Marie and Madrona, Jam Krista and Naag, Mark Frances},
  options = {useprefix=true},
  date = {2021-02-11},
  institution = {{Bicol Unviersity}},
  location = {{Legazpi City, Albay}},
  abstract = {Hydrodistillation of Canarium luzonicum resin, commonly known as Manila elemi, was studied. Distillation conditions were run at different distillation times, method of extraction (water distillation; Water and steam distillation), the number of trays. Interpretation of laboratory results was done through kinetic modeling and percent yield of elemi oil. Results showed that the distillation followed first order kinetics and the most effective way of distillation was water and steam with two layers giving a percent yield of 11.08 percent. The index of refraction, color, and density of the extracted elemi oil was determined to be 1.4764, light yellow, and 0.833 g/mL, respectively. The researchers designed and fabricated an essential oil distiller based on the laboratory findings. Efficiency of the distiller was tested set of trials with six different conditions. Results showed that air dried and uncomminuted resin with four trays in water and steam distillation gave the highest percent yield with 11.41 percent. The researchers concluded that, based on the findings, more trays give higher yield, ensuring a more intimate contact with the steam. Extraction time ranges from 1 to 1.5 hours. The computed rate of investment in the payback and the payback period are 326.481\% and 0.30 months respectively.},
  langid = {english},
  pagetotal = {92},
  keywords = {FAV}
}

@incollection{alexander2001,
