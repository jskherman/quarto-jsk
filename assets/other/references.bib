  pages = {210--247},
  publisher = {{Sociedade Brasileira de Ciência e Tecnologia de Alimentos; https://web.archive.org/web/20220131082341/http://www.scielo.br/j/cta/a/7Fzm7Gnkm7STdFSYykXLqqs/?lang=en; https://web.archive.org/web/20220131082429/http://www.scielo.br/j/cta/a/7Fzm7Gnkm7STdFSYykXLqqs/?lang=en}},
