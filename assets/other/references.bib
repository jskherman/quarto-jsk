@article{ha2008,
  title = {Essential {{Oil}} from {{Lemongrass Extracted}} by {{Supercritical Carbon Dioxide}} and {{Steam Distillation}}},
  author = {Ha, Huynh K. P. and Maridable, J. and Gaspillo, P. and Hasika, M. and Malaluan, R. and Kawasaki, J. and Uplb, Ovcre},
  date = {2008},
  journaltitle = {The Philippine Agricultural Scientist},
  volume = {91},
  number = {1; https://web.archive.org/web/20220121151429/https://ovcre.uplb.edu.ph/journals-uplb/index.php/PAS/article/view/7},
  issn = {0031-7454},
  url = {https://ovcre.uplb.edu.ph/journals-uplb/index.php/PAS/article/view/7},
  urldate = {2022-01-21},
  abstract = {This study compares the composition of essential oil extracted from lemongrass leaves and stems using supercritical CO2 (SC CO2) and steam distillation. In the process using SC CO2 extraction in a Supercritical Fluid Extraction System (SFE), the temperature of extraction was varied from 35–50 0C, while the pressure applied was 9.1–11.1 MPa. The flow rate of CO2 to the reactor was maintained at 0.5 m3 h-1. Steam distillation was conducted using a standard bench scale setup. The extracts from both methods were analyzed by gas chromatography - mass spectrometry (GC-MS) and the variations of the composition were reported. The study showed that better oil quality in terms of composition was produced from the air dried raw materials than from direct heat drying. The essential oil extracted from air dried lemongrass leaves by SFE process contained 94.4\% citral, 1.14\% myrcene and 0.5\% limonene, showing a composition similar to the reference standard. In contrast, the oil from the air dried lemongrass stems fell short in terms of purity. SFE was found to be a superior process than steam distillation, producing better quality essential oil containing 90\% Citral.},
  issue = {1},
  langid = {english},
  keywords = {archived,essential oil,lemongrass,Supercritical Carbon Dioxide Extraction},
  file = {E\:\\OneDrive\\Documents\\Archive\\zotero\\Ha_etal_2008_Essential_Oil_from_Lemongrass_Extracted_by_Supercritical_Carbon_Dioxide_and.pdf}
}

@article{hamidah2021,
  title = {Corrosion of Copper Alloys in {{KOH}}, {{NaOH}}, {{NaCl}}, and {{HCl}} Electrolyte Solutions and Its Impact to the Mechanical Properties},
  author = {Hamidah, Ida and Solehudin, Agus and Hamdani, Aam and Hasanah, Lilik and Khairurrijal, Khairurrijal and Kurniawan, Tedi and Mamat, Rizalman and Maryanti, Rina and Nandiyanto, Asep Bayu Dani and Hammouti, Belkheir},
  date = {2021-04-01},
  journaltitle = {Alexandria Engineering Journal},
  shortjournal = {Alexandria Engineering Journal},
  volume = {60},
  number = {2},
  pages = {2235--2243},
  issn = {1110-0168},
  doi = {10.1016/j.aej.2020.12.027},
  url = {https://www.sciencedirect.com/science/article/pii/S1110016820306761},
  urldate = {2022-09-06},
  abstract = {Effects of electrolyte solution (i.e., KOH, NaOH, NaCl, and HCl) with various concentrations (from 0 to 0.71~M) on corrosion and mechanical properties of the copper alloys were investigated. Experimental results showed that the increases in the concentration of electrolytes allowed faster corrosion process. However, too high concentration of electrolytes has no further impacts on the corrosion profile due to the existence of the passivity phenomena on the surface of the material, preventing the material to get more corroded. Acidic (HCl) and salt (NaCl) electrolytes gave more impacts on the corrosion compared to the alkaline solution (i.e., NaOH and KOH) since the acidic and salt ions are able to destroy the formed passivation layer. The correlation of the corrosion rate and the mechanical properties was also presented, in which the increases in the corrosion rate were in line with the decreases in the mechanical properties (i.e., tensile and yield strength). During the corrosion, the atomic structure in the material received attacks from the electrolyte ions, making the structure inside the copper alloys to be less strong and easily fatigued. Understanding the corrosion process and mechanical properties is important for further applications of copper-related alloys in extreme and severe conditions.},
  langid = {english},
  keywords = {Copper alloys,Corrosion,Education,Tensile strength,Yield strength},
  file = {E\:\\OneDrive\\Documents\\Archive\\zotero\\Hamidah_etal_2021_Corrosion_of_copper_alloys_in_KOH,_NaOH,_NaCl,_and_HCl_electrolyte_solutions.pdf;C\:\\Users\\Je Sian Keith Herman\\Zotero\\storage\\JNPWXIGF\\S1110016820306761.html}
}

